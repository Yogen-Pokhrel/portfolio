{
  "realm": "KEYCLOAK_PORTFOLIO",
  "auth-server-url": "KEYCLOAK_SERVER_URL",
  "resource": "KEYCLOAK_CLIENT",
  "credentials": {
    "secret": "KEYCLOAK_CLIENT_SECRET"
  },
  "path-cache": {
    "lifespan": 0,
    "max-entries": 0
  },
  "http-method-as-scope": true,
  "paths" : [
    {
      "path": "/swagger-ui/*",
      "enforcement-mode": "DISABLED"
    },
    {
      "path": "/v3/api-docs/*",
      "enforcement-mode": "DISABLED"
    },
    {
      "path": "/*",
      "enforcement-mode": "ENFORCING"
    }
  ]
}